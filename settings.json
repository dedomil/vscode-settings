{
  "breadcrumbs.enabled": false,
  "C_Cpp.formatting": "vcFormat",
  "codesnap.containerPadding": "2px",
  "codesnap.showWindowControls": false,
  "codesnap.showWindowTitle": false,
  "codesnap.shutterAction": "copy",
  "codesnap.transparentBackground": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line",
  "editor.cursorSurroundingLines": 5,
  "editor.cursorWidth": 2,
  "editor.detectIndentation": false,
  "editor.fontFamily": "'Operator Mono'",
  "editor.fontSize": 16,
  "editor.formatOnSave": true,
  "editor.glyphMargin": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.lineHeight": 0,
  "editor.lineNumbers": "interval",
  "editor.minimap.enabled": false,
  "editor.mouseWheelZoom": true,
  "editor.renderWhitespace": "boundary",
  "editor.scrollbar.horizontalScrollbarSize": 3,
  "editor.scrollbar.verticalScrollbarSize": 3,
  "editor.semanticHighlighting.enabled": false,
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "files.autoSave": "afterDelay",
  "fonted.font": "Operator Mono",
  "git.confirmSync": false,
  "git.ignoreMissingGitWarning": true,
  "git.suggestSmartCommit": false,
  "gitlens.views.contributors.avatars": false,
  "gitlens.views.contributors.showStatistics": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "json.schemaDownload.enable": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "telemetry.telemetryLevel": "off",
  "terminal.explorerKind": "external",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.defaultLocation": "editor",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.fontFamily": "'Operator Mono'",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.tabs.defaultIcon": "github",
  "thunder-client.saveRequestOnSend": false,
  "window.commandCenter": false,
  "window.menuBarVisibility": "compact",
  "window.title": "Code",
  "window.zoomLevel": 2,
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Base16 Leaf Tomorrow",
  "workbench.editor.untitled.hint": "hidden",
  "workbench.layoutControl.enabled": false,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.statusBar.visible": false
}